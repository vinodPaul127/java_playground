import java.util.*;
import java.lang.*;
import java.io.*;
import java.time.*;
import java.time.format.DateTimeFormatter;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 LocalDateTime myDateobj = LocalDateTime.now();
	 System.out.println("unformatted" +   myDateobj);
	 DateTimeFormatter MyFormateobj = DateTimeFormatter.ofPattern("dd-MM-yyyy HH:mm:ss");
	 String formattedDate = myDateobj.format(MyFormateobj);
	 System.out.println("Formatted" + formattedDate);
    
	}
}
